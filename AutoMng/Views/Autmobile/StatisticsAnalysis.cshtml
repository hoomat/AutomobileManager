@model List<AutomobilMng.Models.AutomobileModel>

<div class="row-fluid">
    <div class="breadcrumbs" id="breadcrumbs">
        <div class="btn-group @AVAResource.Resource.Pull_Dir ">
            <button class="btn btn-pink " id="fueldistance">
                <i class=" fa fa-search "></i>
                @AVAResource.Resource.Chart_Fuel_Distance
            </button>
            <button class="btn btn-success " id="repairbuy">
                <i class=" fa fa-search "></i>
                @AVAResource.Resource.Chart_Repiar_Buy
            </button>
        </div>
    </div>
</div>
<div class="row-fluid loader" data-initialize="loader" id="content"/>

<script type="text/javascript">
    var spinner; 
    $('#fueldistance').click(function () {
        Sppiner();
        var url = "@Url.Action("ChartCompareFuelDistance", "Autmobile")";
        var containerholder = $("#content");
        $.ajax({
            cache: false,
            type: "GET",
            url: url,
            success: function (data) {
                containerholder.html('');
                containerholder.html(data);
                spinner.stop();
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert('Error: ' + xhr.statusText);
                spinner.stop();
            }
        });
    });
</script>
